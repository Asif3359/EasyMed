<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.*?>
<VBox xmlns:fx="http://javafx.com/fxml"
      xmlns="http://javafx.com/javafx"
      fx:id="sidebar" alignment="TOP_CENTER" prefHeight="731.0" prefWidth="233.0"
      style="-fx-background-color: #089bab; -fx-background-radius: 0 20 20 0;"
      fx:controller="com.easymed.controllers.doctor.SidebarController">
    <ImageView fx:id="logo" fitHeight="51.0" fitWidth="153.0" pickOnBounds="true" preserveRatio="true">
        <VBox.margin>
            <Insets top="14.0"/>
        </VBox.margin>
        <Image url="@../../images/logo.png"/>
    </ImageView>
    <JFXButton fx:id="dashboard" graphicTextGap="25.0" onAction="#dashboard" prefHeight="54.0" prefWidth="234.0"
               styleClass="sidebarBtn" text="Dashboard">
        <graphic>
            <ImageView fitHeight="39.0" fitWidth="90.0" pickOnBounds="true" preserveRatio="true">
                <Image url="@../../images/dashboard_layout_96px.png"/>
            </ImageView>
        </graphic>
        <font>
            <Font size="20.0"/>
        </font>
        <VBox.margin>
            <Insets top="20.0"/>
        </VBox.margin>
        <padding>
            <Insets bottom="10.0" right="15.0" top="10.0"/>
        </padding>
    </JFXButton>
    <JFXButton fx:id="profile" graphicTextGap="25.0" layoutX="10.0" layoutY="91.0" onAction="#profile" prefHeight="54.0"
               prefWidth="234.0" text="Profile" styleClass="sidebarBtn">
        <font>
            <Font size="20.0"/>
        </font>
        <VBox.margin>
            <Insets/>
        </VBox.margin>
        <padding>
            <Insets bottom="10.0" right="53.0" top="10.0"/>
        </padding>
        <graphic>
            <ImageView fitHeight="39.0" fitWidth="90.0" pickOnBounds="true" preserveRatio="true">
                <Image url="@../../images/profile_96px.png"/>
            </ImageView>
        </graphic>
    </JFXButton>
    <JFXButton fx:id="doctors" graphicTextGap="25.0" layoutX="10.0" layoutY="155.0" onAction="#doctors"
               prefHeight="54.0"
               prefWidth="234.0" text="Doctors" styleClass="sidebarBtn">
        <font>
            <Font size="20.0"/>
        </font>
        <padding>
            <Insets bottom="10.0" right="39.0" top="10.0"/>
        </padding>
        <graphic>
            <ImageView fitHeight="39.0" fitWidth="90.0" pickOnBounds="true" preserveRatio="true">
                <Image url="@../../images/MedicalDoctor_512px.png"/>
            </ImageView>
        </graphic>
    </JFXButton>
    <JFXButton fx:id="patients" graphicTextGap="25.0" layoutX="10.0" layoutY="214.0" onAction="#patients"
               prefHeight="54.0"
               prefWidth="234.0" text="Patients">
        <graphic>
            <ImageView fitHeight="37.0" fitWidth="34.0" pickOnBounds="true" preserveRatio="true">
                <Image url="@../../images/sick_208px.png"/>
            </ImageView>
        </graphic>
        <font>
            <Font size="20.0"/>
        </font>
        <padding>
            <Insets bottom="10.0" right="39.0" top="10.0"/>
        </padding>
    </JFXButton>
    <JFXButton fx:id="appointments" graphicTextGap="25.0" layoutX="10.0" layoutY="273.0" onAction="#appointments"
               prefHeight="54.0"
               prefWidth="234.0" text="Appointments">
        <graphic>
            <ImageView fitHeight="37.0" fitWidth="34.0" pickOnBounds="true" preserveRatio="true">
                <Image url="@../../images/appointment_scheduling_96px.png"/>
            </ImageView>
        </graphic>
        <font>
            <Font size="20.0"/>
        </font>
        <padding>
            <Insets bottom="10.0" right="-11.0" top="10.0"/>
        </padding>
    </JFXButton>
    <JFXButton fx:id="logout" graphicTextGap="25.0" layoutX="10.0" layoutY="273.0" onAction="#logout" prefHeight="54.0"
               prefWidth="234.0" text="Logout" styleClass="sidebarBtn">
        <graphic>
            <ImageView fitHeight="39.0" fitWidth="90.0" pickOnBounds="true" preserveRatio="true">
                <Image url="@../../images/logout_480px.png"/>
            </ImageView>
        </graphic>
        <font>
            <Font size="20.0"/>
        </font>
        <padding>
            <Insets bottom="10.0" right="46.0" top="10.0"/>
        </padding>
    </JFXButton>
</VBox>
